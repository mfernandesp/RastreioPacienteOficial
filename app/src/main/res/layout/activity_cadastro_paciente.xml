            tools:layout_editor_absoluteY="332dp" tools:layout_editor_absoluteX="99dp"/>
    <TextView
            android:text="Telefone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            tools:layout_editor_absoluteY="313dp" tools:layout_editor_absoluteX="99dp"/>
    <TextView
            android:text="Plano de SaÃºde"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            tools:layout_editor_absoluteY="390dp" tools:layout_editor_absoluteX="102dp"/>
    <TextView
            android:text="CPF"
            android:layout_width="26dp"
            android:layout_height="39dp"
            android:id="@+id/textView8"
            tools:layout_editor_absoluteY="454dp" tools:layout_editor_absoluteX="101dp"/>
