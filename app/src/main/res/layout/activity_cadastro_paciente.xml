            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/cpfTextCardastrarPaciente"
            tools:layout_editor_absoluteY="493dp" tools:layout_editor_absoluteX="101dp"/>
    <Button
            android:text="Cadastrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/cadastrarPaciente"
            tools:layout_editor_absoluteY="562dp" tools:layout_editor_absoluteX="207dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/planoSaudeTextCardastrarPaciente"
            tools:layout_editor_absoluteY="409dp" tools:layout_editor_absoluteX="101dp"/>

</android.support.constraint.ConstraintLayout>